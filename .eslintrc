{
  "root": true,
  "env": {
    "node": true,
    "webextensions": true
  },
  "extends": ["plugin:vue/recommended", "@vue/standard", "@vue/prettier"],
  "rules": {
    "max-len": ["error", 120],
    "vue/singleline-html-element-content-newline": "off",
    "vue/multiline-html-element-content-newline": "off"
  }
}
