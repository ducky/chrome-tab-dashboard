{
  "name": "Clear Skies",
  "version": "0.0.3",
  "manifest_version": 2,
  "description": "Beautiful 'New Tab' page that helps you stay on time the current time, weather, and pretty colors",
  "homepage_url": "http://christieman.com/",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "dist/index.html"
  },
  "permissions": ["geolocation", "storage"]
}
